package ISC;

public class TipeData {
    public static void main(String[] args) {
        //Tipe data primitf
    
        //Tipe data bilangan bulat
        byte angka1 = 1;
        short angka2 = 2;
        int angka3 = 3;
        long angka4 = 4;

        //Tipe data bilangan desimal
        float angka5 = 5.1f;
        double angka6 = 6.1;
        
        //Tipe data karakter
        char karakter = 'a';
        
        //Tipe data logika
        boolean logika = true;
        
        //Tipe data non-primitf
        String nama = "Arhamullah";
        
        //Menampilkan atau memanggil data yang disimpan dalam variabel
        System.out.println("Ini adalah tipe data byte: "+angka1);
        System.out.println("Ini adalah tipe data short: "+angka2);
        System.out.println("Ini adalah tipe data integer: "+angka3);
        System.out.println("Ini adalah tipe data long: "+angka4);
        System.out.println("Ini adalah tipe data float: "+angka5);
        System.out.println("Ini adalah tipe data double: "+angka6);
        System.out.println("Ini adalah tipe data char: "+karakter);
        System.out.println("Ini adalah tipe data boolean: "+logika);
        System.out.println("Ini adalah tipe data String: "+nama);
    }
}
